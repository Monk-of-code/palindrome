#include <stdio.h>
int main()
{
    char s1[30],s2[30];
    int c;
    printf("\n enter the string  (s1)\n");
    scanf("%s",&s1);
    strcpy(s2,s1);
    strrev (s2);
    c=strncmp(s1,s2);
    printf("\n reverse string s2:%s",s2);
    if(c==0)
    {
        printf("\n %s is a palindrome\n ",s1);
    }else{

        printf("\n %s is not a palindrome\n",s2);
    }
       return 0;
    }
